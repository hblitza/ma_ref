@article{cuddington_process-based_2013,
  langid = {english},
  title = {Process-Based Models Are Required to Manage Ecological Systems in a Changing World},
  volume = {4},
  issn = {2150-8925},
  url = {https://esajournals.onlinelibrary.wiley.com/doi/abs/10.1890/ES12-00178.1},
  doi = {10.1890/ES12-00178.1},
  abstract = {Several modeling approaches can be used to guide management decisions. However, some approaches are better fitted than others to address the problem of prediction under global change. Process-based models, which are based on a theoretical understanding of relevant ecological processes, provide a useful framework to incorporate specific responses to altered environmental conditions. As a result, these models can offer significant advantages in predicting the effects of global change as compared to purely statistical or rule-based models based on previously collected data. Process-based models also offer more explicitly stated assumptions and easier interpretation than detailed simulation models. We provide guidelines for identifying the appropriate type of model and level of complexity for management decisions. Finally we outline some of those factors that make modeling for local and regional management under global change a particular challenge: changes to relevant scales and processes, additional sources of uncertainty, legacy effects, threshold dynamics, and socio-economic impacts.},
  number = {2},
  journaltitle = {Ecosphere},
  urldate = {2018-10-04},
  date = {2013-02-01},
  pages = {art20},
  keywords = {climate change,expert opinion,extrapolation,simulation model},
  author = {Cuddington, K. and Fortin, M.-J. and Gerber, L. R. and Hastings, A. and Liebhold, A. and O'Connor, M. and Ray, C.},
  file = {/home/hannes/.mozilla/firefox/pvwmjt0v.default/zotero/storage/NV4GFHM5/Cuddington et al. - 2013 - Process-based models are required to manage ecolog.pdf;/home/hannes/.mozilla/firefox/pvwmjt0v.default/zotero/storage/MIS3DD2I/ES12-00178.html}
}

